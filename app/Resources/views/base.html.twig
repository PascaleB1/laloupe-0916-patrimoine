            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
            <![endif]-->
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/patrimoine/css/style.css') }}" />
        {% endblock %}
        {#<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />#}
    </head>


{% block navbar %}
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"> <span class="sr-only"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="accueil.html">Acceuil</a></li>
                    <li class="active"><a href="associations.html">Associations</a></li>
                    <li class="active"><a href="histoire.html">Histoire</a></li>
                    <li class="active"><a href="realisations.html">Realisations</a></li>
                    <li class="active"><a href="agenda.html">Agenda</a></li>
                    <li class="active"> <a class="btn btn-outline-warning" href="mecene.html" role="button">Devenir mecene</a></li>
                </ul>
            </div>
            <!--/.nav-collapse -->
        </div>
    </nav>
    {% endblock %}

    {% block body %}
       {% endblock %}

        <footer>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xs-4">
                        <p id="float_right">Réalise par: </p>
                    </div>
                    <div class="col-xs-4">
                        <a class="btn btn-primary text-center" role="button" data-toggle="modal" data-target="#myModal"><img src="{{ asset('bundles/patrimoine/img/WCS-logo.png') }}" alt="log_WCS" /></a>
                    </div>
                    <div class="col-xs-4"><a href="#">Mentions legales</a></div>
                </div>
            </div>
            <!-- Modal -->
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="myModalLabel">Site réalisé par:</h4> </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-xs-4"> <a class="btn btn-default" href="{{ asset('https://www.linkedin.com/in/pascale-barboteau-4a0409a5') }}" target="_blank" role="button">Pascale</a> </div>
                                <div class="col-xs-4"> <a class="btn btn-default" href="{{ asset('https://www.linkedin.com/in/lionel-picard') }}" target="_blank" role="button">Lionel</a> </div>
                                <div class="col-xs-4"> <a class="btn btn-default" href="{{ asset('https://fr.linkedin.com/in/yannick-le-corre-96019812a') }}" target="_blank" role="button">Yannick</a> </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-4"><img src="{{ asset('bundles/patrimoine/img/BARBOTEAU%20Pascale.JPG') }}" alt="Pascale" class="thumbnail eleve" /></div>
                                <div class="col-xs-4"><img src="{{ asset('bundles/patrimoine/img/Picard%20Lionel.JPG') }}" alt="Pascale" class="thumbnail eleve" /></div>
                                <div class="col-xs-4"><img src="{{ asset('bundles/patrimoine/img/Le%20Corre%20Yannick.JPG') }}" alt="Pascale" class="thumbnail eleve" /></div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <div class="row">
                                <div class="col-xs-12"> <a class="btn btn-default" href="{{ asset('http://www.wildcodeschool.fr') }}" target="_blank" role="button">contacter l'école</a> </div>
                            </div>
                            <button type="button" class="btn btn-default" data-dismiss="modal">Fermer</button>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

        {% block javascripts %}
            <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        <!-- Include all compiled plugins (below), or include individual files as needed -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    {% endblock %}
    </body>
</html>
