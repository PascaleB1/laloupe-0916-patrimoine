<?php

namespace PatrimoineBundle\Entity;

/**
 * Association
 */
class Association
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $intro;

    /**
     * @var string
     */
    private $article;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set intro
     *
     * @param string $intro
     *
     * @return Association
     */
    public function setIntro($intro)
    {
        $this->intro = $intro;

        return $this;
    }

    /**
     * Get intro
     *
     * @return string
     */
    public function getIntro()
    {
        return $this->intro;
    }

    /**
     * Set article
     *
     * @param string $article
     *
     * @return Association
     */
    public function setArticle($article)
    {
        $this->article = $article;

        return $this;
    }

    /**
     * Get article
     *
     * @return string
     */
    public function getArticle()
    {
        return $this->article;
    }
}

